# Add this at the end of the SCsub file
Import('env')

module_name = 'bimdf'

sources = [
    'bimdf.cpp',
    'register_types.cpp'
]

env.Append(CPPPATH=['#modules/libsatsuma/src'])
env.Append(CPPPATH=['#modules/libsatsuma/thirdparty/lemon'])
