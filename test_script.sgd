# Simple SafeGDScript test to verify GDExtension migration
# This script creates a node and manipulates it to test the sandbox

extends Node

var test_node: Node

func _ready():
    print("SafeGDScript test starting...")

    # Create a test node
    test_node = Node.new()
    test_node.name = "TestNode"
    add_child(test_node)

    print("Created node: ", test_node.name)
    print("Node is inside tree: ", test_node.is_inside_tree())

    # Test property access
    test_node.set("name", "RenamedNode")
    print("Renamed to: ", test_node.get("name"))

    print("SafeGDScript test completed successfully!")
